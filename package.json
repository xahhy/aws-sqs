{
  "name": "aws-sqs-cli",
  "version": "1.0.2-rc5",
  "description": "A CLI For AWS SQS Service Workshop",
  "main": "src/index.js",
  "bin": {
    "aws-sqs-cli": "bin/aws-sqs-cli"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "start": "node src/index.js",
    "dev": "node --inspect src/index.js",
    "test": "jest --coverage"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint:fix && npm run test",
      "pre-push": "npm run lint && npm run test"
    }
  },
  "lint-staged": {
    "**/*.js": [
      "npm run lint:fix",
      "git add"
    ]
  },
  "keywords": [
    "aws",
    "sqs",
    "tutorial"
  ],
  "author": "Herb <1014596312@qq.com>",
  "license": "MIT",
  "dependencies": {
    "aws-sdk": "2.533.0",
    "chalk": "2.4.2",
    "dotenv": "8.1.0",
    "esm": "3.2.25",
    "inquirer": "7.0.0",
    "marked": "0.7.0",
    "marked-terminal": "3.3.0",
    "node-emoji": "1.10.0",
    "prettyjson": "1.2.1",
    "sqs-consumer": "5.4.0",
    "terminal-image": "0.2.0"
  },
  "devDependencies": {
    "@babel/plugin-transform-modules-commonjs": "7.6.0",
    "babel-eslint": "10.0.3",
    "codecov": "3.6.1",
    "eslint": "6.4.0",
    "eslint-config-airbnb-base": "14.0.0",
    "eslint-config-google": "0.14.0",
    "eslint-config-prettier": "6.4.0",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-prettier": "3.1.1",
    "husky": "3.0.8",
    "jest": "24.9.0",
    "lint-staged": "9.4.2",
    "prettier": "1.18.2",
    "sinon": "7.5.0"
  },
  "repository": {
    "url": "https://github.com/xahhy/aws-sqs.git"
  },
  "engines": {
    "node": ">=8.1.4"
  }
}
